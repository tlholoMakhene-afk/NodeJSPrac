<!DOCTYPE html>

<html >
<head>
    <meta charset="UTF-8">
    <title>Animal Management System</title>
    <style>
        body { padding-top:50px; }
    </style>
</head>
<body>
    <header>
        <nav >
            <a  href="/">Animal Management System</a>
          </nav>
    </header>
    <table>
        <tr>
            <th>Name</th>
            <th>Type</th>
            <th>Food</th>
            <th>Age</th>
        </tr>
        <% for (var i = 0; i < data.length; i++) { %>
                <tr>
                    <td><%=data[i].Name %></td>
                    <td><%=data[i].Type %></td>
                    <td><%=data[i].Food %></td>
                    <td><%=data[i].Age %></td>
                </tr>
        <%}%>
    </table>
    
</body>
</html>